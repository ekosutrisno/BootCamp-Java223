package Pertemuan1;

public class Latihan5 {

	public static void main(String[] args) {
		int kali;
		for (int a = 0; a <= 6; a++) {
			for (int b = 0; b <= 6; b++) {
				if (a == 0) {
					System.out.print(b+"   ");
				} else if (a == b) {
					System.out.println(a);
				}else if(a != b) {
					kali = a * b;
					System.out.print(kali);
				}else if(b!=a) {
					kali = b *a;
					System.out.print(kali);
				}
				

			}
			System.out.println();
		}
	}
}
