package ArrayPackage;

public class Array1 {

	int[] x = new int[10];

	public void printArray(int[] array) {
		for (int i = 0; i < array.length; i++) {
			System.out.print(array[i] + " ");
		}
	}

	public void printArray(String[] array) {
		for (int i = 0; i < array.length; i++) {
			System.out.print(array[i] + " ");
		}
	}

	public void String printArray2() {
		Array2 arr = new Array2();
		return arr.str;
	}

	public void printArrayMD(int[] arr) {
		for (int a = 0; a < matrikA.length; a++) {

			for (int b = 0; b < matrikA[a].length; b++) {
				System.out.print(matrikA[a][b]);
			}
			System.out.println();
		}

	}
}
