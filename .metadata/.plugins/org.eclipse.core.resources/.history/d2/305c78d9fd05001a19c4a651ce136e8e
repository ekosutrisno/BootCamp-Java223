package Pertemuan1;

public class Latihan3 {

	public static void main(String[] args) {
		int jumlah = 0;
		String hasil ="";
		int b = 0, c = 0;
		for (int a = 1; a <= 10; a++) {
			if (a % 2 == 1) {
				b = a;
		
			}
			if (a % 2 == 0) {
				c = a;
				
			}
			jumlah = b + c;

			//System.out.println(b + " + " + c + " = " + jumlah);
		}
	}
}
