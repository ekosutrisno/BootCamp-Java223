package ArrayPackage;

import java.util.Scanner;

public class Array4 {

	public int[][] inputMatrik(int[][] arr) {
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr[i].length; j++) {
Scanner scn = new Scanner(System.in);
System.out.print("Element Matrik ke ["+i+"]["+j+"] : ");
			}
		}
	}

}
