package Pertemuan1;

public class Latihan2 {

	public static void main(String[] args) {

		jumlahAngka();
		System.out.println();
		kurangAngka();

	}

	private static void jumlahAngka() {
		int jumlah = 0;
		for (int a = 1; a <= 5; a++) {
			System.out.println(a);
			jumlah = a;
		}
	}

	private static void kurangAngka() {
		for (int a = 5; a > 0; a--) {
			System.out.println(a);
		}
	}

}
