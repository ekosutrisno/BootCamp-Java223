package LatihanSoal;

import java.util.Scanner;

public class Soal1 {
	private static Scanner inputUser;

	public static void main(String[] args) {
		inputUser = new Scanner(System.in);
		System.out.print("Masukkan Angka: ");
		int inputAngka = inputUser.nextInt();

		System.out.println("Faktor Dari " + inputAngka + " adalah " + pohonFakor(inputAngka));

	}

	public static int pohonFakor(int angka) {
		int faktor = 0;
		for (int i = 1; i <= angka; i++) {
			faktor++;
			if (angka % faktor == 0) {
				int[] hasil_faktrornya = new int[i];
				hasil_faktrornya[i] = faktor++;
			}
		}
		int[] hasil_faktrornya;
		return hasil_faktrornya;
	}

}
