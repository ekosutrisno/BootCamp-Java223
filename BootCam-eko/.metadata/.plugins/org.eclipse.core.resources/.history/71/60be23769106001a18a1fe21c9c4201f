package Pertemuan1;

public class Latihan5 {

	public static void main(String[] args) {
		int kali;
		for (int a = 0; a <= 6; a++) {
			for (int b = 0; b <= 6; b++) {
				if (a == 0&&b==0) {
					System.out.print(b + "   ");
				} else if (a == 0&&b!=0) {
					System.out.println(b);
				} else if (a!=0&&b==0) {
					System.out.print(a);
				} else {
					int x=a*b;
					System.out.print(x+" ");
				}
			}
			System.out.println();
		}
	}
}
