package ArrayPackage;
import java.util.*;
public class Array1 {

	int[] x = new int[10];

	public void printArray(int[] array) {
		for (int i = 0; i < array.length; i++) {
			System.out.print("[");
			System.out.print(array[i] + "\t");
			System.out.print("]");
		}
	}

	public String printArray2() {
		Array2 arr = new Array2();
		return arr.str;
	}

}
